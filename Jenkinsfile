pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Code') {
            steps {
                echo 'Code'
            }
        }
        stage('Build') {
            steps {
                echo 'Build'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy'
            }
        }
        stage('Test') {
            steps {
                echo 'Test'
            }
        }
    }
    node {
        stage('Hello'){
        echo 'Hello World'
         }
        stage('Code'){
        echo 'Code'
        }
        
        stage('Build') {
            echo 'Build'
        }
        stage('Deploy') {
        echo 'Deploy'
        }
        stage('Test') {
        echo 'Build'
       }
  
}
